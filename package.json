    "scripts": {
        "dev": "vite",
        "dev:debug": "node --trace-warnings scripts/dev-debug.mjs",
        "dev:3000": "vite --host localhost --port 3000 --strictPort",
        "build": "npm run og:image && vite build && node scripts/emit-asset-manifest.mjs",
        "build:clean": "rimraf build && vite build",
        "build:pages": "npm run og:image && cross-env VITE_BASE=/zenotika/ vite build && node scripts/copy-404.mjs && node scripts/emit-asset-manifest.mjs",
        "preview": "vite preview --open",
        "preview:pages": "vite preview --outDir build --port 3000 --strictPort --host localhost",
        "preview:tunnel": "vite preview --outDir build --port 3000 --strictPort --host localhost",
        "a11y:preview": "node scripts/a11y-preview.mjs",
        "a11y:pages": "wait-on http://localhost:3000 && node scripts/a11y-preview.mjs",
        "a11y:one": "concurrently -k -s first -n serve,a11y \"npm run preview:pages\" \"npm run a11y:pages\"",
        "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
        "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:e2e": "playwright test",
        "test:e2e:pwa": "cross-env VITE_ENABLE_PWA=true VITE_E2E_PWA=true npm run build && cross-env E2E_PWA=true VITE_ENABLE_PWA=true VITE_E2E_PWA=true npx playwright test --config playwright.pwa.config.ts",
        "bundle:check": "node scripts/check-bundle.js",
        "baseline:collect": "node scripts/collect-baselines.mjs",
        "baseline:flags:on": "cross-env VITE_ENABLE_PERF_METRICS=true VITE_ENABLE_MONITORING=true npm run baseline:collect",
        "analyze": "vite build --mode analyze",
        "ci:local": "npm run lint && npm run test && npm run build && npm run bundle:check && npm run baseline:collect",
        "pwa:icons": "node scripts/generate-pwa-icons.mjs",
        "og:image": "node scripts/generate-og-image.mjs",
        "lint:colors": "node scripts/lint-colors.mjs"
    }
}